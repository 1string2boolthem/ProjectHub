<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>GetUserProjectsServlet</servlet-name>
    <servlet-class>GetUserProjectsServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>GetUserProjectsServlet</servlet-name>
        <url-pattern>/getUserProjects</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetIndividualsServlet</servlet-name>
        <servlet-class>GetIndividualsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetIndividualsServlet</servlet-name>
        <url-pattern>/getIndividuals</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetResources</servlet-name>
        <servlet-class>GetResources</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetResources</servlet-name>
        <url-pattern>/getResources</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateProjectServlet</servlet-name>
        <servlet-class>CreateProjectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateProjectServlet</servlet-name>
        <url-pattern>/createProject</url-pattern>
    </servlet-mapping>
</web-app>